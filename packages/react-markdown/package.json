  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rehype": "^13.0.2",
    "rehype-pretty-code": "^0.14.0",
    "rehype-react": "^8.0.0",
    "remark-gfm": "^4.0.0",
